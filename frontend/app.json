{
  "expo": {
    "name": "Gerenciador de Eventos",
    "scheme": "gerenciador-eventos",
    "slug": "gerenciador-eventos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#2edae0"
    },
     "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gabrielsoaresgomes.gerenciadoreventos"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabrielsoaresgomes.gerenciadoreventos"
    },
    "web": {
      "favicon": "./assets/icon.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "a2302df8-008c-4468-8981-4eea3541d283"
      }
    }
  }
}
